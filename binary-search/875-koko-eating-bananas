class Solution:
    def minEatingSpeed(self, piles: List[int], h: int) -> int:
        low, high = 1, max(piles)
        result = high

        while low <= high:
            mid = (low + high) // 2

            total_time = 0
            for pile in piles:
                total_time += math.ceil(float(pile) / mid)
            if total_time <= h:
                result = mid
                high = mid - 1
            else:
                low = mid + 1
        return result
            