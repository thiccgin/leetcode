class Solution:
    def isValid(self, s: str) -> bool:
        if len(s) % 2 != 0:
            return False

        stack = []
        close_to_open = {
            ')': '(',
            ']': '[',
            '}': '{'
        }

        for c in s:
            if c in close_to_open.keys():
                if stack and stack[-1] == close_to_open[c]:
                    stack.pop()
                else:
                    return False
            else:
                stack.append(c)
                
        return True if not stack else False