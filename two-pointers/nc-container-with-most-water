class Solution:
    def maxArea(self, heights: List[int]) -> int:
        L = 0
        R = len(heights) - 1
        max_water = 0

        while L < R:
            curr_water = (R - L) * min(heights[L], heights[R])
            max_water = max(max_water, curr_water)
            if heights[L] <= heights[R]:
                L += 1
            else:
                R -= 1
        return max_water